{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ant-design-pro": "^2.3.2",
    "antd": "^3.19.3",
    "rc-queue-anim": "^1.6.12",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typeface-open-sans": "0.0.54",
    "nodemon": "^1.17.5",
    "express": "^4.16.3",
    "cors": "^2.8.4",
    "body-parser": "^1.18.3",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  },
  "scripts": {
    "server": "./node_modules/.bin/nodemon --watch server --exec babel-node -- server/index.js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "cp config.js ./src && npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
